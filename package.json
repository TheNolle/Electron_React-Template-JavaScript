{
  "name": "electron-x-react",
  "version": "0.0.1",
  "main": "electron-main.js",
  "scripts": {
    "start": "pnpm build && electron .",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"pnpm build\" \"webpack serve --config webpack.config.js --mode development --port 45678\" \"wait-on http://localhost:45678 && electron .\"",
    "build": "webpack --config webpack.config.js --mode production",
    "package": "pnpm build && electron-builder"
  },
  "build": {
    "appId": "com.thenolle.app.electron-x-react",
    "productName": "electron-x-react",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "dist/",
      "main.js"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^24.2.0",
    "electron-builder": "^23.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "style-loader": "^3.3.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3"
  }
}